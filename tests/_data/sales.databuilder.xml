<?xml version="1.0"?>
<transfers
    xmlns="spryker:databuilder-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>

    <transfer name="Item">
        <property name="unitNetPrice" dataBuilderRule="randomNumber(5)"/>
        <property name="unitSubtotalAggregation" dataBuilderRule="randomNumber(5)"/>
        <property name="unitDiscountAmountAggregation" dataBuilderRule="randomNumber(5)"/>
        <property name="unitTaxAmountFullAggregation" dataBuilderRule="randomNumber(5)"/>
        <property name="sumSubtotalAggregation" dataBuilderRule="randomNumber(5)"/>
        <property name="sumDiscountAmountFullAggregation" dataBuilderRule="randomNumber(5)"/>
        <property name="sumTaxAmountFullAggregation" dataBuilderRule="randomNumber(5)"/>
    </transfer>

    <transfer name="Expense">
        <property name="sumDiscountAmountAggregation" dataBuilderRule="randomNumber(5)"/>
    </transfer>

</transfers>
